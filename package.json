{
  "name": "elysia-http-problem-json",
  "version": "0.1.0",
  "type": "module",
  "module": "src/index.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.js",
    "node": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "prepare": "husky || true",
    "prepublishOnly": "HUSKY=0 && bun test && bun tsc",
