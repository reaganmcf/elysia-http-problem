    "lint": "biome lint --fix && biome format --fix"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "husky": "^9.1.7",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "elysia": ">=1.4.11"
  },
  "keywords": ["elysia", "problem", "json", "http", "bun"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reaganmcf/elysia-http-problem-json"
  }
}
